{
    "name": "betterunitcorrector",
    "version": "1.0.0",
    "description": "UnitCorrector for Discord, but better",
    "scripts": {
        "lint": "eslint ./src/**/* --ext .ts --fix",
        "predev": "npm run lint",
        "dev": "ts-node-dev --cls --notify=false --inspect -T -r ./src/config/dotenv.ts ./src/bot.ts",
        "prebuild": "npm run lint",
        "build": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jonnopon3000/BetterUnitCorrector.git"
    },
    "bugs": {
        "url": "https://github.com/jonnopon3000/BetterUnitCorrector/issues"
    },
    "author": "null",
    "homepage": "https://github.com/jonnopon3000/BetterUnitCorrector#readme",
    "dependencies": {
        "discord.js": "^12.3.1",
        "dotenv-safe": "^8.2.0",
        "module-alias": "^2.2.2"
    },
    "devDependencies": {
        "@types/dotenv-safe": "^8.1.1",
        "@types/module-alias": "^2.0.0",
        "@types/node": "^14.11.2",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "eslint": "^7.10.0",
        "ts-node-dev": "^1.0.0-pre.63",
        "typescript": "^4.0.3"
    }
}